{
  "name": "astro-port-generator-interactive",
  "version": "1.0.0",
  "description": "Interactive visualization tool for Middleman to Astro port process",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --exec ts-node server/index.ts",
    "server:build": "tsc && node dist/server/index.js",
    "client": "cd client && npm run dev",
    "build": "tsc && cd client && npm run build",
    "start": "node dist/server/index.js",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "chokidar": "^3.5.3",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/fs-extra": "^11.0.4"
  },
  "keywords": ["astro", "middleman", "port", "visualization"],
  "author": "",
  "license": "MIT"
}